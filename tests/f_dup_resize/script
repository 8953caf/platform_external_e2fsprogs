SKIP_GUNZIP="true"

touch $TMPFILE
mke2fs -F -O resize_inode $TMPFILE 10240 > /dev/null 2>&1
debugfs -w $TMPFILE << "EOF" > /dev/null 2>&1
freeb 4 4
freeb 8195 4
write /sbin/debugfs debugfs
set_inode_field debugfs mtime 200504110000
q
EOF

E2FSCK_TIME=1113200000
export E2FSCK_TIME

. $cmd_dir/run_e2fsck

unset E2FSCK_TIME
